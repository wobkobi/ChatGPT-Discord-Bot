{
  "name": "chatgpt",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --src src",
    "dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm/transpile-only src/index.ts\"",
    "start": "npm run build && node build/index.js",
    "watch": "nodemon --exec npm run dev --watch src --ext ts"
  },
  "dependencies": {
    "@discordjs/rest": "^2.4.3",
    "@giphy/js-fetch-api": "^5.6.0",
    "discord-api-types": "^0.38.1",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "logform": "^2.7.0",
    "luxon": "^3.5.0",
    "mathjax-full": "^3.2.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.85.1",
    "sanitize-html": "^2.16.0",
    "sharp": "^0.34.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.20.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.4",
    "@types/sanitize-html": "^2.15.0",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,scss,html,json,yml,yaml,md,mdx}": "prettier --write"
  }
}
